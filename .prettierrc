{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["./node_modules/prettier-plugin-sort-imports/dist/index.js"],
  "sortingMethod": "alphabetical",
  "importTypeOrder": ["NPMPackages", "localImports"],
  "newlineBetweenTypes": true
}
